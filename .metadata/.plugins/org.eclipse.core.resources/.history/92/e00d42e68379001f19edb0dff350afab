package com.cursoJava.cuenta;

public class Main {
	    public static void main(String[] args) {
	        // Crear una cuenta con solo el titular
	        Cuenta cuenta1 = new Cuenta("Juan Pérez");
	        System.out.println(cuenta1);  

	        // Ingresar dinero en la cuenta
	        cuenta1.ingresar(100);
	        System.out.println(cuenta1);  

	        // Retirar una cantidad mayor a la que tiene
	        cuenta1.retirar(150);
	        System.out.println(cuenta1);  
	        // Crear otra cuenta con titular y cantidad inicial
	        Cuenta cuenta2 = new Cuenta("Ana López", 200);
	        System.out.println(cuenta2);  

	        // Ingresar dinero
	        cuenta2.ingresar(50);
	        System.out.println(cuenta2); 

	        // Retirar una cantidad dentro de lo que tiene
	        cuenta2.retirar(100);
	        System.out.println(cuenta2);  
	    }
	}

