
public class Main {
    public static void main(String[] args) {
        // Crear un ordenador
        Ordenador ordenador = new Ordenador("001", "HP", 800.0, 10, "Intel i5", 8, 512);
        System.out.println(ordenador);

        // Crear un teléfono móvil
        Telefono movil = new Telefono("002", "Samsung Galaxy", 999 , 5, "Movistar");
        System.out.println(movil);
    }
}
