package com.cursoJava.cuenta;

public class Cuenta {
	String titular;
	double cantidad;
	
	
	public Cuenta (String titular) {
		this.titular = titular;
		this.cantidad = 0;
	}


	public Cuenta(String titular, double cantidad) {
		this.titular = titular;
		this.cantidad = cantidad;
		
		if(cantidad < 0) {
			this.cantidad = 0; 
			}else {
				this.cantidad = cantidad;
		}
	}
	
	

}
